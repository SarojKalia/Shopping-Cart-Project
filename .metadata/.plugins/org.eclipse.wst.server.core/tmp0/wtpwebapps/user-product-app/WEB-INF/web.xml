<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>LoginUserServlet</servlet-name>
		<display-name>LoginUserServlet</display-name>
		<description></description>
		<servlet-class>org.jsp.userProductApp.controller.LoginUserServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<display-name>LoginServlet</display-name>
		<description></description>
		<servlet-class>org.jsp.userProductApp.controller.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ViewProductServlet</servlet-name>
		<display-name>ViewProductServlet</display-name>
		<description></description>
		<servlet-class>org.jsp.userProductApp.controller.ViewProductServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>EditProductServlet</servlet-name>
		<display-name>EditProductServlet</display-name>
		<description></description>
		<servlet-class>org.jsp.userProductApp.controller.EditProductServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DeleteProductServlet</servlet-name>
		<display-name>DeleteProductServlet</display-name>
		<description></description>
		<servlet-class>org.jsp.userProductApp.controller.DeleteProductServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginUserServlet</servlet-name>
		<url-pattern>/LoginUserServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/LoginServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ViewProductServlet</servlet-name>
		<url-pattern>/ViewProductServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>EditProductServlet</servlet-name>
		<url-pattern>/EditProductServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DeleteProductServlet</servlet-name>
		<url-pattern>/DeleteProductServlet</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>login.jsp</welcome-file>
	</welcome-file-list>
</web-app>
